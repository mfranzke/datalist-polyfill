{
  "name": "datalist-polyfill",
  "version": "1.24.3",
  "description": "A minimal and dependency-free vanilla JavaScript datalist polyfill. Supports all standard's functionality as well as mimics other browsers behavior.",
  "main": "datalist-polyfill.js",
  "scripts": {
    "test": "xo datalist-polyfill.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mfranzke/datalist-polyfill.git"
  },
  "author": "Maximilian Franzke",
  "license": "MIT",
  "readmeFilename": "README.md",
  "keywords": [
    "datalist",
    "polyfill",
    "javascript",
    "html",
    "autosuggest",
    "autosuggester",
    "suggest",
    "suggester",
    "select"
  ],
  "bugs": {
    "url": "https://github.com/mfranzke/datalist-polyfill/issues"
  },
  "homepage": "https://github.com/mfranzke/datalist-polyfill",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@wdio/cli": "^5.22.4",
    "@wdio/dot-reporter": "^5.22.4",
    "@wdio/local-runner": "^5.22.4",
    "@wdio/mocha-framework": "^6.0.1",
    "@wdio/sync": "^5.20.1",
    "husky": "^4.2.3",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "webdriverio": "^5.22.4",
    "xo": "^0.28.0"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "prettier": true,
    "esnext": false,
    "rules": {
      "one-var": 0,
      "no-use-before-define": 0,
      "unicorn/prefer-includes": 0,
      "unicorn/prefer-dataset": 0
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
